<div class="card-header d-flex justify-content-between">
  <span>
  <%= @mycelium.name %> Petri Runs
  </span>
  <%= link_to 'New Petri run', new_mycelium_petri_run_path(params[:mycelium_id]), class: 'btn btn-info btn-sm' %>
  <%= link_to 'Back', @mycelium, class: 'btn btn-info btn-sm' %>
</div>


  <ul class='list-group'>
    <% @petri_runs.each do |petri_run| %>
      <li class='list-group-item'>
        <%= link_to petri_run.strain, [petri_run.mycelium, petri_run] %>
      </li>
    <% end %>
  </ul>
